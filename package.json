  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "deploy": "gatsby clean && gatsby build --prefix-paths && touch public/CNAME && echo 'perky.design' > public/CNAME && cp README.md public && cp LICENSE public && gh-pages -d public -b master"
  },
  "lint-staged": {
    "ignore": [
      "node_modules",
      "dist",
      "package-lock.json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
